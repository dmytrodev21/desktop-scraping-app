@model k190144_Q4.Models.CategoryViewModel;

<p><b>All categories</b></p>

<table>
    <tr>
        <th>Script</th>
        <th>CurrentPrice</th>
    </tr>
    @foreach (var script in Model.scripts)
    {
        <tr>
            <td>@script.Script</td>
            <td>@script.CurrentPrice</td>
        </tr>
    }
</table>
